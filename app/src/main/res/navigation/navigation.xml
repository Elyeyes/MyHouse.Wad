<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/roomsFragment">
    <fragment
        android:id="@+id/roomsFragment"
        android:name="com.mathieu_elyes.housewad.DevicesFragment"
        android:label="fragment_rooms"
        tools:layout="@layout/fragment_devices" >
        <action
            android:id="@+id/action_roomsFragment_to_menuFragment"
            app:destination="@id/menuFragment" />
    </fragment>
    <fragment
        android:id="@+id/modesFragment"
        android:name="com.mathieu_elyes.housewad.ModesFragment"
        android:label="fragment_modes"
        tools:layout="@layout/fragment_modes" />
    <fragment
        android:id="@+id/guestFragment"
        android:name="com.mathieu_elyes.housewad.GuestFragment"
        android:label="fragment_guest"
        tools:layout="@layout/fragment_guest" />
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.mathieu_elyes.housewad.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" />
</navigation>